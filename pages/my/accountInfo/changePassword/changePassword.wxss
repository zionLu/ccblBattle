/* pages/my/accountInfo/changePassword/changePassword.wxss */
.wrapper{
    width: 100%;
    height: 100%; 
}
.part{
    width: 100%;
    height: 200rpx;
    background: white;
    margin-top: 30rpx;
}
.upPart{
    height: 100rpx;
}
.cell{
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    position: relative;
}
.title{
    height: 100rpx;
    width: 160rpx;
    line-height: 100rpx;
    margin-left: 32rpx;
    font-size: 34rpx;
    color: #282828
}
.input{
    width: 279rpx;
    height: 100rpx;
    line-height: 100rpx;
    margin-left: 62rpx;
    font-size: 34rpx;

}
.newPswInput,.confirmInput{
    width: 390rpx;
}
.wrong{
    width: 37rpx;
    height: 37rpx;
}
.getVerifyButton{
    background: #4556a0;
    font-size: 24rpx;
    color: white;
    width: 180rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 10rpx;
}
.verifyTitle::before{
    content: none;
    /* background: #cccccc;
    width: 650rpx;
    height: 1rpx;
    position: absolute;
    right: 0;
    top: 0; */
}
.confirmTitle::before{
    content: ' ';
    background: #cccccc;
    width: 650rpx;
    height: 1rpx;
    position: absolute;
    right: 0;
    top: 0;
}
.submit{
    width: 660rpx;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    margin: 0 auto;
    margin-top: 60rpx;
    background: #4556a0;
    color: white;
    font-size: 34rpx;
    border-radius: 10rpx;
    
}