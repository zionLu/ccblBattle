.wrapper{
    width: 100%;
    height: 100%;
}
.photo{
    width: 100%;
    height:400rpx;
    display: block;
}
.addressForm{
    margin-top: 40rpx;
    width: 100%;
    background: white;
}
.dateForm{
    margin-top: 30rpx;
    width: 100%;
    background: white;
}
.formItem{
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
    position: relative;
}
.buttonBlock{
    margin: 0 auto;
    margin-top: 60rpx;
    width: auto;
    text-align: center;
    margin-bottom: 60rpx;
}
.button{
    display: inline-block;
    width: 216rpx;
    height: 66rpx;
    line-height: 66rpx;
    text-align: center;
    font-size: 34rpx;
    color: #4556a0;
    background: white;
    border: 2rpx solid #4556a0;
    border-radius: 25rpx;
}
.submit{
    margin-left: 153rpx;
    background: #4556a0;
    color: white;
}
.addressNameIcon{
    width: 26rpx;
    height: 36rpx;
}
.title{
    width: 167rpx;
    height: 100rpx;
    font-size: 34rpx;
    line-height: 100rpx;
    text-align: right;
    color: #4556a0;
}
.contant{
    width: 519rpx; 
    margin-left: 64rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    color: #b5b5b5;
    /* border-bottom: 1rpx solid #f7f7f7;  */
}
.addressNameContant{
    width: 444rpx;
}
.addressPositionContant{
    overflow: hidden;
    color: #909090;
}
.contant::after{
    content: ' ';
    display: block;
    background: #dfdfdf;
    width: 619.2rpx;
    height: 2rpx;
    position: absolute;
    right: 0;
    bottom: 0;
}

.addressDescriptionContant::after,.timeContant::after{
    content: none;
}
input{
    height: 100rpx;
    line-height: 100rpx;
}
